{
    "name": "artics-ada-gnat-vscode",
    "displayName": "ada-gnat",
    "description": "Ada mode for vs code integrated with AdaCore's Gnat",
    "version": "0.0.1",
    "publisher": "pfe",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*",
        "onCommand:adagnat.activate"
    ],
    "main": "./out/ada-gnat",
    "contributes": {
        "commands": [
            {
                "command": "ada-gnat.adaCheck",
                "title": "ada-gnat check extension cmd"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
